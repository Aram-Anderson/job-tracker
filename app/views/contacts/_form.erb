<h5>Add a Contact For This Company</h5>

<%= form_for [ @company, @contact ] do |f| %>
    <%= f.label :name %><br/>
    <%= f.text_field :name %>

    <%= f.label :position %>
    <%= f.text_field :position %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.submit 'Submit' %>

<% end %>
